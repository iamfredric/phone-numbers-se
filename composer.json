{
    "name": "iamfredric/phone-numbers-se",
    "description": "A library for formatting Swedish phone numbers",
    "keywords": [
        "phone-number"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Fredric",
        "role": "Developer"
    }],
    "autoload": {
        "psr-4": {
            "Iamfredric\\Phone\\": "src/"
        }
    },
    "require": {
        "php": "^8.1|^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "phpstan/phpstan": "^1.4",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "analyze": "vendor/bin/phpstan analyse",
        "sniff": "vendor/bin/phpcs --standard=PSR2 --encoding=utf-8 -p --colors ./src",
        "fix": "vendor/bin/phpcbf --standard=PSR2 --encoding=utf-8 -p --colors ./src"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
